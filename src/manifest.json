{
  "name": "西门问雪",
  "appid": "__UNI__XIMENCUIXUE",
  "description": "下雪查找小程序 — 帮助用户快速查找当前哪些地方正在下雪或即将下雪",
  "versionName": "1.0.0",
  "versionCode": "100",
  "transformPx": false,
  "mp-weixin": {
    "__usePrivacyCheck__": true,
    "appid": "wx0000000000000000",
    "setting": {
      "urlCheck": false,
      "es6": true,
      "enhance": true,
      "postcss": true,
      "minified": true,
      "newFeature": true,
      "bigPackageSizeSupport": true
    },
    "usingComponents": true,
    "lazyCodeLoading": "requiredComponents",
    "optimization": {
      "subPackages": true
    },
    "componentFramework": "glass-easel",
    "rendererOptions": {
      "skyline": {
        "defaultDisplayBlock": true,
        "disableABTest": true,
        "sdkVersionBegin": "3.0.0",
        "sdkVersionEnd": "15.255.255"
      }
    },
    "cloudfunctionRoot": "cloudfunctions/",
    "permission": {
      "scope.userLocation": {
        "desc": "用于获取附近降雪地点信息"
      }
    }
  },
  "h5": {
    "devServer": {
      "port": 8080,
      "disableHostCheck": true
    },
    "router": {
      "mode": "hash"
    }
  }
}
